#include "hw.h"





void hwInit(void)
{
  bspInit();

  cliInit(); /* 다른 모듈 보다 가장 위에서 초기화 해줘야함 */
  ledInit();
  usbInit();
  uartInit();
  buttonInit();
  gpioInit();

  if (sdInit() == true)
  {
	 fatfsInit();
  }

  usbBegin(USB_CDC_MODE);
}
